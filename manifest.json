{
    "manifest_version": 3,
    "name": "Dole Bludger",
    "version": "1.0",
    "description": "Easily fill out your job search on the workforceaustralia.gov.au website",
    "host_permissions": [
        "https://www.workforceaustralia.gov.au/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.workforceaustralia.gov.au/*",
                "https://www.seek.com.au/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}